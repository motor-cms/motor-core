{
    "name": "motor-cms/motor-core",
    "description": "Motor base package",
    "license": "MIT",
    "keywords": [
        "motor"
    ],
    "authors": [
        {
            "name": "Reza Esmaili",
            "email": "me@dfox.info"
        }
    ],
    "require": {
        "php": "^8.4",
        "dedoc/scramble": "^0.12.19",
        "laravel/fortify": "^1.25",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10",
        "meilisearch/meilisearch-php": "^1.12"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-laravel": "^3.0",
        "barryvdh/laravel-ide-helper": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Motor\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Motor\\Core\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Motor\\Core\\Providers\\MotorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    }
}
