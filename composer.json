{
    "name": "motor-cms/motor-core",
    "description": "Motor base package",
    "license": "MIT",
    "keywords": [
        "motor"
    ],
    "authors": [
        {
            "name": "Reza Esmaili",
            "email": "me@dfox.info"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0",
        "laravel/tinker": "^2.10",
        "meilisearch/meilisearch-php": "^1.12"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "laravel/pint": "^1.19",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.5",
        "spatie/laravel-ignition": "^2.9",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-laravel": "^3.0",
        "barryvdh/laravel-ide-helper": "^3.5",
        "itsgoingd/clockwork": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Motor\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Motor\\Core\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Motor\\Core\\Providers\\MotorServiceProvider",
                "Motor\\Core\\Providers\\AwsS3NoCredentialsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    }
}
